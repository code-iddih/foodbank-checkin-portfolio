{
  "name": "foodbank-admin-panel",
  "version": "1.0.0",
  "description": "Foodbank Check-In and Appointment System - Admin Panel",
  "type": "module",
  "private": true,
  "author": "Lindsey D. Stead / Lifesaver Technology Services",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run",
    "test:integration": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
